extends ../shared/template
block content
  .row
    .col-xs-12
      #map
    .col-xs-12
      table.blue(class='table table-striped')
        thead
          th Name
          th Location
          th Difficulty
          th Found?
        tbody
        each treasure in treasures
          tr(data-name=treasure.name, data-lat=treasure.lat, data-lng=treasure.lng)
            td= treasure.name
            td= treasure.loc
            td= treasure.difficulty 
            td 


block scripts
          script(src='/js/user/index.js')
